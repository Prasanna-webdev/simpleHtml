            <!-- Table Tag -->
            <!-- Table Tag -->
<h2>Table Tag</h2>
<table>
    <tr>
        <th colspan="2">Table Header 1</th>
        <th>Table Header 2</th>
    </tr>
    <tr>
        <td rowspan="2">Merged Cells</td>
        <td>Row 1, Cell 2</td>
        <td>Row 1, Cell 3</td>
    </tr>
    <tr>
        <td>Row 2, Cell 2</td>
        <td>Row 2, Cell 3</td>
    </tr>
    <!-- Additional Row -->
    <tr>
        <td colspan="3">Row 3, Cells 1-3</td>
    </tr>
</table>


            <!-- Embedding Content -->
            <h2>Embedding Content</h2>
